PATH=/bin:/usr/bin:/usr/local/bin
MAILDIR=$HOME/mail
DEFAULT=$MAILDIR/
VERBOSE=ON
LOGFILE=$MAILDIR/log

:0
* ^Subject:.*电话录音数据
${MAILDIR}/phone_data/

:0
* ^From.*system@vcchina.com
${MAILDIR}/oa/

:0
* ^From.*git@fangde.com
${MAILDIR}/gitlab/


:0
* ^To:*@fangde.com
${MAILDIR}/fangde/

:0
* ^From:*@fangde.com
${MAILDIR}/fangde/

# 剩下的邮件存放至inbox目录中
:0
* .*
${MAILDIR}/inbox/
